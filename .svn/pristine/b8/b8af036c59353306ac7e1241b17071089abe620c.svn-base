package com.hadoit.game.fsgame.gms.meta;

public class TotalChargeData {
	private int serverId;
	private String oprGroup;
	private long chargeAmount;

	public TotalChargeData() {

	}

	public TotalChargeData(int serverId, String oprGroup, long chargeAmount) {
		this.serverId = serverId;
		this.oprGroup = oprGroup;
		this.chargeAmount = chargeAmount;
	}

	public int getServerId() {
		return serverId;
	}

	public void setServerId(int serverId) {
		this.serverId = serverId;
	}

	public String getOprGroup() {
		return oprGroup;
	}

	public void setOprGroup(String oprGroup) {
		this.oprGroup = oprGroup;
	}

	public long getChargeAmount() {
		return chargeAmount;
	}

	public void setChargeAmount(long chargeAmount) {
		this.chargeAmount = chargeAmount;
	}

}
